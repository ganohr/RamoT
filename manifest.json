{
	"name": "RamoT",
	"description": "__MSG_extensionDescription__",
	"version": "0.0.2",
	"manifest_version": 3,
	"homepage_url": "https://ganohr.net/blog/ramot",
	"content_scripts": [
		{
			"matches": [
				"https://twitter.com/*",
				"https://mobile.twitter.com/*"
			],
			"js": [
				"content.js"
			],
			"default_icon": "icon-16x16.png"
		}
	],
	"icons": {
		"16": "icon-16x16.png",
		"32": "icon-32x32.png",
		"64": "icon-64x64.png",
		"128": "icon-128x128.png"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage"
	],
	"web_accessible_resources": [
		{
			"resources": [
				"content.css"
			],
			"matches": [
				"https://twitter.com/*",
				"https://mobile.twitter.com/*"
			]
		}
	],
	"default_locale": "ja",
	"update_url": "https://raw.githubusercontent.com/ganohr/RamoT/main/updates.xml",
	"key": ""
}